var a = prompt(«Запишите первое число», 1);
var b = prompt(«Запишите второе число», 10);
var c = prompt(«Запишите третье число», 100);
min = Math.min(a, b, c);
max = Math.max(a, b, c);
alert(«Минимум из » + a + «, » + b + «, » + c + » равно » + min);
alert(«Максимум из » + a + «, » + b + «, » + c + » равно » + max);
